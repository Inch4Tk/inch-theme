{
  "$schema": "vscode://schemas/color-theme",
  "name": "Inch Theme",
  "type": "dark",
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "module": {
      "foreground": "#93a9d1"
    },
    "variable.defaultLibrary": {
      "foreground": "#93a9d1"
    },
    "class.builtin": {
      "foreground": "#56b6c2"
    },
    "function.declaration": {
      "foreground": "#5bb0f5"
    },
    "variable.declaration": {
      "foreground": "#d3dce9"
    },
    "variable.readonly": {
      "foreground": "#e6d2d2"
    },
    "variable.readonly:python": {
      "foreground": "#da8536"
    },
    "selfParameter": {
      "foreground": "#e5c07b"
    },
    "property": {
      "foreground": "#f0d7d2"
    },
    "property:python": {
      "foreground": "#C6CCD7"
    },
    "macro": {
      "foreground": "#697fe0"
    }
  },
  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#5F6672"
      }
    },
    {
      "scope": ["entity.name.function", "support.function", "meta.function-call"],
      "settings": {
        "foreground": "#61afef"
      }
    },
    {
      "scope": [
        "keyword",
        "punctuation.definition.keyword",
        "variable.language",
        "storage.type.modifier",
        "storage.modifier",
      ],
      "settings": {
        "foreground": "#c678dd"
      }
    },
    {
      "scope": [
        "keyword.operator",
        "storage.type.string.python",
        "punctuation.separator.pointer-access"
      ],
      "settings": {
        "foreground": "#f07474"
      }
    },
    {
      "scope": [
        "keyword.codetag.notation"
      ],
      "settings": {
        "foreground": "#c609ff"
      }
    },
    {
      "scope": ["constant.other.caps"],
      "settings": {
        "foreground": "#da8536"
      }
    },
    {
      "scope": ["constant", "constant.language", "support.constant"],
      "settings": {
        "foreground": "#d19a66"
      }
    },
    {
      "scope": ["constant.numeric", "support.constant.numeric"],
      "settings": {
        "foreground": "#f1c28b"
      }
    },
    {
      "scope": ["storage", "support.class"],
      "settings": {
        "foreground": "#e5c07b"
      }
    },
    {
      "scope": ["markup.inline.raw.string", "string"],
      "settings": {
        "foreground": "#98C379"
      }
    },
    {
      "scope": ["markup.fenced_code"],
      "settings": {
        "foreground": "#f1ead2"
      }
    },
    {
      "scope": [
        "entity.name.section",
        "entity.name.tag",
        "entity.name.type",
        "support.type"
      ],
      "settings": {
        "foreground": "#56b6c2"
      }
    },
    {
      "scope": ["entity.name.type.parameter", "support.type.property-name", "support.variable", "variable", "source"],
      "settings": {
        "foreground": "#C6CCD7"
      }
    },
    {
      "scope": [
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#abbbe2"
      }
    },
    {
      "scope": [
        "entity.other",
        "punctuation.definition.entity",
        "support.other"
      ],
      "settings": {
        "foreground": "#D19A66"
      }
    },
    {
      "scope": ["meta.brace", "punctuation", "keyword.other.unit.suffix.floating-point"],
      "settings": {
        "foreground": "#737983"
      }
    },
    {
      "name": "bold",
      "scope": [
        "markup.bold",
        "punctuation.definition.bold",
        "entity.other.attribute-name.id"
      ],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "italic",
      "scope": ["comment", "markup.italic", "punctuation.definition.italic", "variable.parameter"],
      "settings": {
        "fontStyle": "italic"
      }
    }
  ],
  "colors": {
    "activityBar.background": "#282c34",
    "activityBar.foreground": "#d7dae0",
    "activityBarBadge.background": "#4d78cc",
    "activityBarBadge.foreground": "#f8fafd",
    "badge.background": "#282c34",
    "button.background": "#404754",
    "debugToolBar.background": "#21252b",
    "descriptionForeground": "#abb2bf",
    "diffEditor.insertedTextBackground": "#00809b33",
    "dropdown.background": "#21252b",
    "dropdown.border": "#21252b",
    "editor.background": "#282c34",
    "editor.findMatchBackground": "#42557b",
    "editor.findMatchBorder": "#457dff",
    "editor.findMatchHighlightBackground": "#6199ff2f",
    "editor.foreground": "#abb2bf",
    "editor.lineHighlightBackground": "#2c313c",
    "editorLineNumber.activeForeground": "#abb2bf",
    "editorGutter.addedBackground": "#109868",
    "editorGutter.deletedBackground": "#9A353D",
    "editorGutter.modifiedBackground": "#948B60",
    "editor.selectionBackground": "#67769660",
    "editor.selectionHighlightBackground": "#ffffff10",
    "editor.selectionHighlightBorder": "#dddddd",
    "editor.wordHighlightBackground": "#d2e0ff2f",
    "editor.wordHighlightBorder": "#7f848e",
    "editor.wordHighlightStrongBackground": "#abb2bf26",
    "editor.wordHighlightStrongBorder": "#7f848e",
    "editorBracketMatch.background": "#515a6b",
